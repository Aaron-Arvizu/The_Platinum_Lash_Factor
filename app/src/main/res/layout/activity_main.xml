<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/signOutBtn"
        android:layout_width="120dp"
        android:layout_height="46dp"
        android:layout_marginStart="621dp"
        android:layout_marginTop="145dp"
        android:layout_marginEnd="59dp"
        android:layout_marginBottom="1089dp"
        android:text="Sign Out"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/clientListBtn"
        android:layout_width="643dp"
        android:layout_height="100dp"
        android:layout_marginStart="79dp"
        android:layout_marginTop="749dp"
        android:layout_marginEnd="78dp"
        android:layout_marginBottom="431dp"
        android:background="@drawable/rounded_corner"
        android:text="client list"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/versionText"
        android:layout_width="125dp"
        android:layout_height="44dp"
        android:layout_marginStart="518dp"
        android:layout_marginTop="668dp"
        android:layout_marginEnd="157dp"
        android:layout_marginBottom="559dp"
        android:text="Version 1.7.0"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/usersName"
        android:layout_width="173dp"
        android:layout_height="68dp"
        android:layout_marginStart="592dp"
        android:layout_marginTop="77dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="1135dp"
        android:freezesText="false"
        android:gravity="center|center_vertical"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="700dp"
        android:layout_height="616dp"
        android:layout_marginStart="157dp"
        android:layout_marginTop="228dp"
        android:layout_marginEnd="157dp"
        android:layout_marginBottom="603dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.568"
        app:srcCompat="@drawable/mainlogo" />

    <Button
        android:id="@+id/lashReleaseBtn"
        android:layout_width="643dp"
        android:layout_height="100dp"
        android:layout_marginStart="79dp"
        android:layout_marginTop="869dp"
        android:layout_marginEnd="78dp"
        android:layout_marginBottom="311dp"
        android:background="@drawable/rounded_corner"
        android:text="Lash Release"
        android:textSize="36sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/addEmpBtn"
        android:layout_width="643dp"
        android:layout_height="100dp"
        android:layout_marginStart="79dp"
        android:layout_marginTop="992dp"
        android:layout_marginEnd="78dp"
        android:layout_marginBottom="188dp"
        android:background="@drawable/rounded_corner"
        android:text="Add Employee"
        android:textSize="36sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>